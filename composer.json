{
  "name": "alizafer/exegeo",
  "description": "ExeGeo - PHP MVC Mini Framework (AkademikX yapısına göre)",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "scripts": {
    "start": "php -S 127.0.0.1:8000 index.php",
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
